function x(x1,x2,x3,x4,x5,x6,x7,x8,x9,x10,x11,x12,x13,x14,x15,x16,x17,x18,x19,x20,x21)
{



top.document.getElementById('myFrames').cols="*,0,0,0)";
top.controller.document.all.mycell1.style.backgroundColor='#8FB5DA';
top.controller.document.all.mycell2.style.backgroundColor='#CFDFEF';
top.controller.document.all.mycell3.style.backgroundColor='#CFDFEF';
top.controller.document.all.mycell4.style.backgroundColor='#CFDFEF';
top.controller.document.all.label1.style.fontWeight = "bold";
top.controller.document.all.label2.style.fontWeight = "normal";
top.controller.document.all.label3.style.fontWeight = "normal";
top.controller.document.all.label4.style.fontWeight = "normal";
top.controller.document.all.label1.innerHTML='SEARCH';



top.x1.document.zabba.p.value = x1;
top.x1.document.zabba.w1b.value = '';

switch(x3)
{
case '1':

choose('list','n');
top.x1.document.zabba.w2.value='';
break;

case '2':
choose('chart','n');
top.x1.document.zabba.w2.value='';
break;

case '3':
choose('collocates','n');
top.x1.document.zabba.w2.value=[x4];

if (x5 == 0)
{
docoll('Lno');
}
else
{
docoll('L' + x5);
}

if (x6 == 0)
{
docoll('Rno');
}
else
{
docoll('R' + x6);
}

break;

case '4':
choose('compare','n');
top.x1.document.zabba.w1b.value = x2;
top.x1.document.zabba.w2.value = x4;
docoll('L' + x5);
docoll('R' + x6);
break;

case '5':
choose('kwic','n');
var slots = x5 + ':' + x6;
switch (slots)
{
case '3:0':
kwicx();
kwicL();
break;
case '0:3':
kwicx();
kwicR();
break;
case '1:2':
kwicx();
kwic(-1,3);
kwic(1,5);
kwic(2,6);
break;

default:
}

// choose('kwic');
break;

default:
}



top.x1.document.zabba.sortBy.options[x21].selected = true;



for (var z = 0; z <= 86; z++)
   {
top.x1.document.zabba.sec1.options[z].selected = false;
   }

for (var z = 0; z <= 86; z++)
   {
top.x1.document.zabba.sec2.options[z].selected = false;
   }

if (x8 != '0' & x8 != '888')
{
top.x1.document.zabba.sec1.options[x8].selected = true;
}
if (x9 != '0')
{
top.x1.document.zabba.sec2.options[x9].selected = true;
}

if (x17 != '0')
{
top.x1.document.zabba.sec1.options[x17].selected = true;
}
if (x18 != '0')
{
top.x1.document.zabba.sec1.options[x18].selected = true;
}
if (x19 != '0')
{
top.x1.document.zabba.sec2.options[x19].selected = true;
}
if (x20 != '0')
{
top.x1.document.zabba.sec2.options[x20].selected = true;
}



top.x1.document.zabba.numhits.value = x14;
top.x1.document.zabba.whatshow.options[x15].selected = true;
top.x1.document.zabba.groupBy.options[x16].selected = true;
top.x1.document.zabba.freq1.value = x10;
top.x1.document.zabba.freq2.value = x11;

if (x12 == '1') {
top.x1.document.zabba.limfreq1.checked = true;
}
else {
top.x1.document.zabba.limfreq1.checked = false;
}

if (x13 == '1') {
top.x1.document.zabba.limfreq2.checked = true;
}
else {
top.x1.document.zabba.limfreq2.checked = false;
}



if (x7 == '1') {
top.x1.document.zabba.showsec.checked = true;
}
else {
top.x1.document.zabba.showsec.checked = false;
}



if (top.x1.document.zabba.chooser.value == 'collocates' && (top.x1.document.getElementById('w2').value == '' || top.x1.document.getElementById('w2').value == '*'))
{
top.x1.document.all.w2.value = '*';
top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;
top.x1.document.zabba.minfreq1.options[0].selected = false;
top.x1.document.zabba.minfreq1.options[1].selected = true;
top.x1.document.zabba.limfreq1.checked = true;
top.x1.document.zabba.freq1.value = '3';
}

if (x8 == '888')
{
top.x1.document.all.VC2.style.display='inline';
top.x1.document.all.isVC.value = 'y';

top.x1.document.zabba.secVC.options[0].selected = true;
top.x1.document.zabba.secVC.options[1].selected = false;

top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;

// top.x1.document.zabba.limfreq1.checked = true;
// top.x1.document.zabba.freq1.value = '7';

// alert('ok1');


top.x1.document.all.sectionsLabel.style.color='#808080';
top.x1.document.all.sectionsLabel.style.padding='0px';
top.x1.document.all.sectionsLabel.style.backgroundColor='#EEF3F9';
top.x1.document.all.sectionsLabel.style.fontWeight = "normal";
top.x1.document.all.sectionsLabel.style.borderWidth='0px';
top.x1.document.all.sectionsLabel.style.borderStyle='none';
top.x1.document.all.sectionsLabel.style.borderColor='black';

}

top.x1.document.zabba.submit();
}

// ######################## ############################

function kwic(x,n)
{
var whicher = 's'+ document.zabba.counter.value;
top.x1.document.getElementById(whicher).value = x;
var whicher1 = 'n'+ n;
top.x1.document.getElementById(whicher1).innerHTML = document.zabba.counter.value;
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#A6FFA6';
top.x1.document.zabba.counter.value++;
}

function kwicL()
{
for (n = 1; n <= 3; n++)
{
var whicher = 's'+ n;
top.x1.document.getElementById(whicher).value = 0;
}
for (n = 1; n <= 7; n++)
{
var whicher1 = 'n'+ n;
top.x1.document.getElementById(whicher1).innerHTML = '-';
}
for (n = 1; n <= 3; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#A6FFA6';
}
for (n = 5; n <= 7; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#FFFFFF';
}
top.x1.document.getElementById('n1').innerHTML = '3';
top.x1.document.getElementById('n2').innerHTML = '2';
top.x1.document.getElementById('n3').innerHTML = '1';
top.x1.document.getElementById('s1').value = -1;
top.x1.document.getElementById('s2').value = -2;
top.x1.document.getElementById('s3').value = -3;
top.x1.document.zabba.counter.value = 4;
}

function kwicR()
{
for (n = 1; n <= 3; n++)
{
var whicher = 's'+ n;
top.x1.document.getElementById(whicher).value = 0;
}
for (n = 1; n <= 7; n++)
{
var whicher1 = 'n'+ n;
top.x1.document.getElementById(whicher1).innerHTML = '-';
}
for (n = 1; n <= 3; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#FFFFFF';
}
for (n = 5; n <= 7; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#A6FFA6';
}
top.x1.document.getElementById('n5').innerHTML = '1';
top.x1.document.getElementById('n6').innerHTML = '2';
top.x1.document.getElementById('n7').innerHTML = '3';
top.x1.document.getElementById('s1').value = 1;
top.x1.document.getElementById('s2').value = 2;
top.x1.document.getElementById('s3').value = 3;
top.x1.document.zabba.counter.value = 4;
}

function kwicx()
{
for (n = 1; n <= 3; n++)
{
var whicher = 's'+ n;
top.x1.document.getElementById(whicher).value = 0;
}
for (n = 1; n <= 7; n++)
{
var whicher1 = 'n'+ n;
top.x1.document.getElementById(whicher1).innerHTML = '-';
}
for (n = 1; n <= 3; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#FFFFFF';
}
for (n = 5; n <= 7; n++)
{
var whicher2 = 'cellc'+ n;
top.x1.document.getElementById(whicher2).style.backgroundColor='#FFFFFF';
}
top.x1.document.zabba.counter.value = 1;
}


function docoll(c)
{


if (c != 'Lmore' && c != 'Rmore')
{
if (c.substring(0,1) == 'L')
{
for (n = 1; n <= 9; n++)
{
var whicher1 = 'cellL'+ n;
top.x1.document.getElementById(whicher1).style.backgroundColor='#EEF3F9';
}
}
else
{
for (n = 1; n <= 9; n++)
{
var whicher1 = 'cellR'+ n;
top.x1.document.getElementById(whicher1).style.backgroundColor='#EEF3F9';
}
}
}

if (c == 'LnoRno')
{
for (n = 1; n <= 9; n++)
{
var whicher1 = 'cellR'+ n;
top.x1.document.getElementById(whicher1).style.backgroundColor='#EEF3F9';
}
}

switch(c)
{

case 'Lmore':
if (top.x1.document.all.cellL5.style.display=='none')
{
top.x1.document.all.cellL5.style.display='table-cell';
top.x1.document.all.cellL6.style.display='table-cell';
}
else
{
top.x1.document.all.cellL7.style.display='table-cell';
top.x1.document.all.cellL8.style.display='table-cell';
top.x1.document.all.cellL9.style.display='table-cell';
}
break;

case 'Rmore':
if (top.x1.document.all.cellR5.style.display=='none')
{
top.x1.document.all.cellR5.style.display='table-cell';
top.x1.document.all.cellR6.style.display='table-cell';
}
else
{
top.x1.document.all.cellR7.style.display='table-cell';
top.x1.document.all.cellR8.style.display='table-cell';
top.x1.document.all.cellR9.style.display='table-cell';
}
break;

case 'Lno':
top.x1.document.all.wl.value = 0;
top.x1.document.all.cellL5.style.display='none';
top.x1.document.all.cellL6.style.display='none';
top.x1.document.all.cellL7.style.display='none';
top.x1.document.all.cellL8.style.display='none';
top.x1.document.all.cellL9.style.display='none';
break;

case 'LnoRno':
top.x1.document.all.wl.value = 0;
top.x1.document.all.cellL5.style.display='none';
top.x1.document.all.cellL6.style.display='none';
top.x1.document.all.cellL7.style.display='none';
top.x1.document.all.cellL8.style.display='none';
top.x1.document.all.cellL9.style.display='none';
top.x1.document.all.wr.value = 0;
top.x1.document.all.cellR5.style.display='none';
top.x1.document.all.cellR6.style.display='none';
top.x1.document.all.cellR7.style.display='none';
top.x1.document.all.cellR8.style.display='none';
top.x1.document.all.cellR9.style.display='none';
break;

case 'L1':
top.x1.document.all.wl.value = 1;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
break;

case 'L2':
top.x1.document.all.wl.value = 2;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
break;

case 'L3':
top.x1.document.all.wl.value = 3;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL3').style.backgroundColor='#A6FFA6';
break;

case 'L4':
top.x1.document.all.wl.value = 4;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL3').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL4').style.backgroundColor='#A6FFA6';
break;

case 'L5':
top.x1.document.all.wl.value = 5;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL3').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL4').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL5').style.backgroundColor='#A6FFA6';
break;

case 'L6':
top.x1.document.all.wl.value = 6;
top.x1.document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
top.x1.document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
