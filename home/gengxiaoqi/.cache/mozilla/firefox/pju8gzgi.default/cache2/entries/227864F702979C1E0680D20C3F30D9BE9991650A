// ##########################  ##########################  CHOOSE

function choose(x,y)
{


if (y != 'n')
{
top.controller.document.all.mycell4.style.border = "thin solid black";
var t=setTimeout("top.controller.document.all.mycell4.style.border = 'none'",1000);
top.controller.document.all.label4.innerHTML='HELP';
}



top.x1.document.all.options2.style.display='none';
