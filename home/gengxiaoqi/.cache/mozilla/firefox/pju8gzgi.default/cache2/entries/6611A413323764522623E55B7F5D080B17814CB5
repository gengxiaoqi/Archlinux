<script language="Javascript">

function mHelp()
{
top.controller.document.all.mycell4.style.backgroundColor='#8FB5DA';
top.controller.document.all.mycell1.style.backgroundColor='#CFDFEF';
top.controller.document.all.mycell2.style.backgroundColor='#CFDFEF';
top.controller.document.all.mycell3.style.backgroundColor='#CFDFEF';
top.controller.document.all.label4.style.fontWeight = "bold";
top.controller.document.all.label1.style.fontWeight = "normal";
top.controller.document.all.label2.style.fontWeight = "normal";
top.controller.document.all.label3.style.fontWeight = "normal";
top.document.getElementById('myFrames').cols="0,0,0,*";
top.controller.document.all.label4.innerHTML = "HELP";
// top.x4.document.location = "help/x1_help.asp?x4=y";
}

function togglePos()
{
if (document.all.posDropdown.style.display=='inline')
{
document.all.posCancel.style.display='none';
document.all.posMake.style.display='inline';
document.all.posDropdown.style.display='none';
}
else
{
document.all.posCancel.style.display='inline';
document.all.posMake.style.display='none';
document.all.posDropdown.style.display='inline';
// alert('ok');
}
}

function toggleDates()
{
if (document.all.calendars.style.display=='inline')
{
document.all.calendars.style.display='none';
document.all.noCalendars.style.display='inline';
}
else
{
document.all.calendars.style.display='inline';
document.all.noCalendars.style.display='none';
// alert('ok');
}
}

function toggleColPos()
{
if (document.all.posColDropdown.style.display=='inline')
{
document.all.posColCancel.style.display='none';
document.all.posColMake.style.display='inline';
document.all.posColDropdown.style.display='none';
}
else
{
document.all.posColCancel.style.display='inline';
document.all.posColMake.style.display='none';
document.all.posColDropdown.style.display='inline';
}
}

function toggleWord2Pos()
{
if (document.all.posWord2Dropdown.style.display=='inline')
{
document.all.posWord2Cancel.style.display='none';
document.all.posWord2Make.style.display='inline';
document.all.posWord2Dropdown.style.display='none';
}
else
{
document.all.posWord2Cancel.style.display='inline';
document.all.posWord2Make.style.display='none';
document.all.posWord2Dropdown.style.display='inline';
}
}

function insertPos()
{
document.zabba.p.value = document.zabba.p.value + '[' + document.zabba.posDropdown.value + '] ';
}

function insertPosCol()
{
document.zabba.w2.value = document.zabba.w2.value + '[' + document.zabba.posColDropdown.value + '] ';
}

function insertWord2Col()
{
document.zabba.w1b.value = document.zabba.w1b.value + '[' + document.zabba.posWord2Dropdown.value + '] ';
}

function togglecollocates()
{
if (document.all.collocatesRow.style.display=='inline')
{
document.all.collocatesRow.style.display='none';
}
else
{
document.all.collocatesRow.style.display='inline';
}
}

