}

function vcdo2a()
{
top.x1.document.all.sortBy.style.backgroundColor = '#FFFFFF'
top.x1.document.all.limfreq1.style.backgroundColor = '#FFFFFF'
top.x1.document.all.freq1.style.backgroundColor = '#FFFFFF'
}

function vcdo2()
{
top.x1.document.all.sortLimit2.style.display='inline';
top.x1.document.all.VC2.style.display='none';
top.x1.document.zabba.sortBy.options[0].selected = false;
top.x1.document.zabba.sortBy.options[1].selected = true;
top.x1.document.zabba.limfreq1.checked = true;
top.x1.document.zabba.freq1.value = '7';
top.x1.document.all.sortBy.style.backgroundColor = '#00FF00'
top.x1.document.all.limfreq1.style.backgroundColor = '#00FF00'
top.x1.document.all.freq1.style.backgroundColor = '#00FF00'
top.x1.document.all.sortLabel.style.color='#000000';
top.x1.document.all.sortLabel.style.padding='3px 8px';
top.x1.document.all.sortLabel.style.backgroundColor='#E0E0E0';
}

