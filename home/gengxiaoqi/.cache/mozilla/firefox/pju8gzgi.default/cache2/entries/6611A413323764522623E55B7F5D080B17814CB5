// ##########################  ##########################  CHOOSE

function choose(x,y)
{



top.x1.document.all.options2.style.display='none';
top.x1.document.all.kwicnum.style.display='none';
top.x1.document.all.optionsa1.style.display='none';
top.x1.document.all.optionsa2.style.display='none';
top.x1.document.all.optionsa3.style.display='none';
top.x1.document.all.optionsa4.style.display='none';

top.x1.document.all.kwicnum.style.display='none';
top.x1.document.zabba.freq2.style.display='none';
top.x1.document.zabba.limfreq2.style.display='none';

top.x1.document.all.label1.style.backgroundColor='#EEF3F9';
top.x1.document.all.label2.style.backgroundColor='#EEF3F9';
top.x1.document.all.label3.style.backgroundColor='#EEF3F9';
top.x1.document.all.label4.style.backgroundColor='#EEF3F9';
top.x1.document.all.label5.style.backgroundColor='#EEF3F9';

top.x1.document.all.label1.style.fontWeight = "normal";
top.x1.document.all.label2.style.fontWeight = "normal";
top.x1.document.all.label3.style.fontWeight = "normal";
top.x1.document.all.label4.style.fontWeight = "normal";
top.x1.document.all.label5.style.fontWeight = "normal";

top.x1.document.all.label1.style.borderWidth='0px';
top.x1.document.all.label2.style.borderWidth='0px';
top.x1.document.all.label3.style.borderWidth='0px';
top.x1.document.all.label4.style.borderWidth='0px';
top.x1.document.all.label5.style.borderWidth='0px';

top.x1.document.all.compareRow.style.display='none';
top.x1.document.all.kwicRow.style.display='none';

if (y != 'n')

{
top.x1.document.all.intro.style.display='none';
top.x1.document.all.help1.style.display='none';
top.x1.document.all.help2.style.display='none';
top.x1.document.all.help3.style.display='none';
top.x1.document.all.help4.style.display='none';
top.x1.document.all.help5.style.display='none';

top.x1.document.all.helpSec.style.display='none';
top.x1.document.all.helpVC.style.display='none';
top.x1.document.all.helpSort.style.display='none';
top.x1.document.all.helpOpt.style.display='none';
}

// set sort etc to default (after compare words)
top.x1.document.zabba.limfreq1.checked = false;
top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.style.backgroundColor='#FFFFFF';
top.x1.document.zabba.freq1.style.backgroundColor='#FFFFFF';
top.x1.document.zabba.limfreq1.style.backgroundColor='#E2ECF5';
top.x1.document.zabba.freq2.style.display='inline';
top.x1.document.zabba.limfreq2.style.display='inline';
top.x1.document.zabba.freq2.style.backgroundColor='#FFFFFF';
top.x1.document.zabba.limfreq2.style.backgroundColor='#E2ECF5';
top.x1.document.getElementById('xrelevance').style.visibility='hidden';
top.x1.document.zabba.sec1.style.backgroundColor='#FFFFFF';
top.x1.document.zabba.sec2.style.backgroundColor='#FFFFFF';
top.x1.document.all.sortLimit2.style.display='none';

top.x1.document.all.sortLabel.style.color='#808080';
top.x1.document.all.sortLabel.style.padding='0px';
top.x1.document.all.sortLabel.style.backgroundColor='#EEF3F9';
top.x1.document.all.sortLabel.style.fontWeight = "normal";
top.x1.document.all.sortLabel.style.borderWidth='0px';
top.x1.document.all.sortLabel.style.borderStyle='none';
top.x1.document.all.sortLabel.style.borderColor='black';

switch(x)
{
// ================================================================ list
case 'list':

top.x1.document.zabba.target='x2';

top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;
top.x1.document.zabba.minfreq1.options[0].selected = true;
top.x1.document.zabba.minfreq1.options[1].selected = false;
top.x1.document.zabba.limfreq1.checked = false;
top.x1.document.zabba.freq1.value = '0';

top.x1.document.all.label1.style.backgroundColor='#DFEAF4';
top.x1.document.all.label1.style.fontWeight = "bold";
top.x1.document.all.label1.style.borderWidth='2px';
top.x1.document.all.label1.style.borderStyle='solid';
top.x1.document.all.label1.style.borderColor='navy';
top.x1.document.all.label1.style.padding='3px';

top.x1.document.zabba.submit1.value='Find matching strings';
top.x1.document.all.myLabel1.innerHTML='';
top.x1.document.zabba.chooser.value='seq';

if (y != 'n')
{
top.x1.document.all.help1.style.display='inline';
top.x1.document.all.w_listx.innerHTML = "";
top.x1.document.all.help1_.style.display='inline';
top.x1.document.all.help1a.style.display='none';
top.x1.document.all.help1b.style.display='none';
top.x1.document.all.help1c.style.display='none';
top.x1.document.all.help1d.style.display='none';
top.x1.document.all.help1e.style.display='none';
top.x1.document.all.help1e_.style.display='none';
top.x1.document.all.help1f.style.display='none';
}

top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;
top.x1.document.all.compareRow.style.display='none';
top.x1.document.all.collocatesRow.style.display='none';
top.x1.document.all.collocatesSpanRow.style.display='none';
top.x1.document.all.kwicRow.style.display='none';

break;

// ================================================================ chart

case 'chart':

if (y != 'n')
{
top.x1.document.all.help2.style.display='inline';
}
top.x1.document.zabba.target='x2';

top.x1.document.all.label2.style.backgroundColor='#DFEAF4';
top.x1.document.all.label2.style.fontWeight = "bold";
top.x1.document.all.label2.style.borderWidth='2px';
top.x1.document.all.label2.style.borderStyle='solid';
top.x1.document.all.label2.style.borderColor='navy';
top.x1.document.all.label2.style.padding='3px';

top.x1.document.zabba.submit1.value='See frequency by section';
top.x1.document.all.myLabel1.innerHTML='';
top.x1.document.zabba.chooser.value='chart';

top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;
top.x1.document.all.compareRow.style.display='none';
top.x1.document.all.collocatesRow.style.display='none';
top.x1.document.all.collocatesSpanRow.style.display='none';
top.x1.document.all.kwicRow.style.display='none';

// alert('ok2');
// top.x1.document.all.help2.style.display='none';

break;

// ================================================================ collocates

case 'collocates':

top.x1.document.zabba.target='x2';

if (y != 'n')
{
top.x1.document.all.help3.style.display='inline';
top.x1.document.all.w_collx.innerHTML = "";
top.x1.document.all.help3.style.display='inline';
top.x1.document.all.help3_.style.display='inline';
top.x1.document.all.help3a.style.display='none';
top.x1.document.all.help3b.style.display='none';
top.x1.document.all.help3c.style.display='none';
top.x1.document.all.help3d.style.display='none';
}

top.x1.document.all.label3.style.backgroundColor='#DFEAF4';
top.x1.document.all.label3.style.fontWeight = "bold";
top.x1.document.all.label3.style.borderWidth='2px';
top.x1.document.all.label3.style.borderStyle='solid';
top.x1.document.all.label3.style.borderColor='navy';
top.x1.document.all.label3.style.padding='3px';

top.x1.document.zabba.submit1.value='Find collocates';
top.x1.document.all.myLabel1.innerHTML='Word/phrase';
top.x1.document.zabba.chooser.value='collocates';

top.x1.document.all.cellL5.style.display='none';
top.x1.document.all.cellL6.style.display='none';
top.x1.document.all.cellL7.style.display='none';
top.x1.document.all.cellL8.style.display='none';
top.x1.document.all.cellL9.style.display='none';
top.x1.document.all.cellR5.style.display='none';
top.x1.document.all.cellR6.style.display='none';
top.x1.document.all.cellR7.style.display='none';
top.x1.document.all.cellR8.style.display='none';
top.x1.document.all.cellR9.style.display='none';

top.x1.document.zabba.sortBy.options[0].selected = true;
top.x1.document.zabba.sortBy.options[1].selected = false;
top.x1.document.all.compareRow.style.display='none';
top.x1.document.all.collocatesRow.style.display='table-row';
top.x1.document.all.collocatesSpanRow.style.display='table-row';
top.x1.document.all.kwicRow.style.display='none';

top.x1.document.all.help1.style.display='none';
top.x1.document.all.help1_.style.display='none';
top.x1.document.all.help1a.style.display='none';
top.x1.document.all.help1b.style.display='none';
top.x1.document.all.help1c.style.display='none';
top.x1.document.all.help1d.style.display='none';
top.x1.document.all.help1e.style.display='none';
top.x1.document.all.help1f.style.display='none';

break;

// ================================================================ compare

case 'compare':

top.x1.document.zabba.limfreq1.checked = true;
top.x1.document.zabba.sortBy.options[1].selected = true;
top.x1.document.zabba.freq2.style.display='inline';
top.x1.document.zabba.limfreq2.style.display='inline';
top.x1.document.zabba.limfreq1.style.backgroundColor='#AEFFAE';
top.x1.document.zabba.freq1.style.backgroundColor='#AEFFAE';
top.x1.document.zabba.limfreq2.style.backgroundColor='#79FF79';
top.x1.document.zabba.freq2.style.backgroundColor='#79FF79';
top.x1.document.getElementById('xrelevance').style.visibility='visible';
top.x1.document.getElementById('xrelevance').innerHTML = "<b>WORD1: WORD2</b>";

top.x1.document.all.sortLabel.style.fontWeight = "bold";
top.x1.document.all.sortLabel.style.borderWidth='2px';
top.x1.document.all.sortLabel.style.borderStyle='solid';
top.x1.document.all.sortLabel.style.borderColor='navy';
top.x1.document.all.sortLabel.style.padding='3px';

top.x1.document.all.sortLabel.style.fontWeight = "bold";
top.x1.document.all.sortLabel.style.borderWidth='2px';
top.x1.document.all.sortLabel.style.borderStyle='solid';
top.x1.document.all.sortLabel.style.borderColor='navy';
top.x1.document.all.sortLabel.style.padding='3px';

var t=setTimeout("top.x1.document.all.sortLabel.style.borderWidth='0px';",1000);
var t=setTimeout("top.x1.document.all.sortLabel.style.fontWeight = 'normal';",1050);



top.x1.document.all.help4.style.display='inline';
top.x1.document.zabba.target='x2';

top.x1.document.all.label4.style.backgroundColor='#DFEAF4';
top.x1.document.all.label4.style.fontWeight = "bold";
top.x1.document.all.label4.style.borderWidth='2px';
top.x1.document.all.label4.style.borderStyle='solid';
top.x1.document.all.label4.style.borderColor='navy';
top.x1.document.all.label4.style.padding='3px';

top.x1.document.zabba.submit1.value='Compare words';
top.x1.document.all.myLabel1.innerHTML='Word1';
top.x1.document.zabba.chooser.value='compareWords';

top.x1.document.all.cellL5.style.display='none';
top.x1.document.all.cellL6.style.display='none';
top.x1.document.all.cellL7.style.display='none';
top.x1.document.all.cellL8.style.display='none';
top.x1.document.all.cellL9.style.display='none';
top.x1.document.all.cellR5.style.display='none';
top.x1.document.all.cellR6.style.display='none';
top.x1.document.all.cellR7.style.display='none';
top.x1.document.all.cellR8.style.display='none';
top.x1.document.all.cellR9.style.display='none';

top.x1.document.zabba.sortBy.options[0].selected = false;
top.x1.document.zabba.sortBy.options[1].selected = true;
top.x1.document.zabba.minfreq1.options[0].selected = true;
top.x1.document.zabba.minfreq1.options[1].selected = false;
top.x1.document.zabba.limfreq1.checked = true;
top.x1.document.zabba.freq1.value = '10';

top.x1.document.zabba.freq2.style.display='inline';
top.x1.document.zabba.limfreq2.style.display='inline';

top.x1.document.all.compareRow.style.display='table-row';
top.x1.document.all.collocatesRow.style.display='table-row';
top.x1.document.all.collocatesSpanRow.style.display='table-row';
top.x1.document.all.kwicRow.style.display='none';

break;

// ================================================================ kwic

case 'kwic':

top.x1.document.all.help5.style.display='inline';
