{
document.all.options.style.display='inline';
}

function resetit()
{
var url = document.location;
// alert(url);
document.location = url;
}

function sec2Selected(x1)
{
if (document.zabba.sec2.options[0].selected == false)
{
document.zabba.showsec.checked = false;
document.zabba.limfreq1.checked = true;
document.zabba.sec1.style.backgroundColor='#AEFFAE';
document.zabba.sec2.style.backgroundColor='#79FF79';
document.zabba.sortBy.options[1].selected = true;
document.zabba.freq2.style.display='inline';
document.zabba.limfreq2.style.display='inline';
document.zabba.limfreq1.style.backgroundColor='#AEFFAE';
document.zabba.freq1.style.backgroundColor='#AEFFAE';
document.zabba.limfreq2.style.backgroundColor='#79FF79';
document.zabba.freq2.style.backgroundColor='#79FF79';
document.getElementById('xrelevance').style.visibility='visible';
document.getElementById('xrelevance').innerHTML = "<b>SEC1 : SEC2</b>";
document.all.sortLimit2.style.display='inline'
}
else
{
document.zabba.limfreq1.checked = false;
document.zabba.sortBy.options[0].selected = true;
document.zabba.sortBy.style.backgroundColor='#FFFFFF';
document.zabba.freq1.style.backgroundColor='#FFFFFF';
document.zabba.limfreq1.style.backgroundColor='#E2ECF5';
document.zabba.freq2.style.display='inline';
document.zabba.limfreq2.style.display='inline';
document.zabba.freq2.style.backgroundColor='#FFFFFF';
document.zabba.limfreq2.style.backgroundColor='#E2ECF5';
document.getElementById('xrelevance').style.visibility='hidden';
document.zabba.sec1.style.backgroundColor='#FFFFFF';
document.zabba.sec2.style.backgroundColor='#FFFFFF';
}
}

function doit()
{
document.zabba.ownsearch.value = 'y';

// alert(document.zabba.kh.value);
if (document.zabba.kh.value > 1000)
{
document.zabba.kh.value = 1000
}



// alert (document.zabba.chooser.value);

if (document.zabba.chooser.value == 'collocates' && document.getElementById('w2').value == '')
{
document.all.w2.value = '*';
document.zabba.freq1.value = '3';

document.all.sortLabel.style.fontWeight = "bold";
document.all.sortLabel.style.borderWidth='2px';
document.all.sortLabel.style.borderStyle='solid';
document.all.sortLabel.style.borderColor='navy';
document.all.sortLabel.style.padding='3px';

document.zabba.limfreq1.style.backgroundColor='#AEFFAE';
document.zabba.freq1.style.backgroundColor='#AEFFAE';
document.getElementById('xrelevance').style.visibility='visible';
document.getElementById('xrelevance').innerHTML = "<b>SEE NOTE</b>";

top.x1.document.all.help4.style.display='inline';

var t=setTimeout("document.all.sortLabel.style.borderWidth='0px';",1000);
var t=setTimeout("document.all.sortLabel.style.fontWeight = 'normal';",1050);

}

if (document.zabba.chooser.value == 'collocates' && document.zabba.sec2.value == '0' && document.getElementById('w2').value == '*')
{
document.zabba.sortBy.options[0].selected = true;
document.zabba.sortBy.options[1].selected = false;
document.zabba.minfreq1.options[0].selected = false;
document.zabba.minfreq1.options[1].selected = true;
document.zabba.limfreq1.checked = true;
}

if (document.zabba.chooser.value == 'collocates' && document.zabba.sec2.value == '0' && (document.getElementById('w2').value == '' || document.getElementById('w2').value == '*'))
{
document.all.w2.value = '*';
document.zabba.sortBy.options[0].selected = true;
document.zabba.sortBy.options[1].selected = false;
document.zabba.minfreq1.options[0].selected = false;
document.zabba.minfreq1.options[1].selected = true;
document.zabba.limfreq1.checked = true;
// document.zabba.freq1.value = '3';
}

if (document.zabba.chooser.value == 'compareWords' && document.getElementById('w2').value == '')
{
document.all.w2.value = '*';
document.zabba.sortBy.options[1].selected = true;
document.zabba.sortBy.options[0].selected = false;
document.zabba.minfreq1.options[0].selected = false;
document.zabba.minfreq1.options[1].selected = true;
document.zabba.limfreq1.checked = true;
document.zabba.freq1.value = '3';
}

if (document.zabba.chooser.value == 'kwic' && document.getElementById('n1').innerHTML == '-' && document.getElementById('n2').innerHTML == '-' && document.getElementById('n3').innerHTML == '-' && document.getElementById('n4').innerHTML == '-' && document.getElementById('n5').innerHTML == '-' && document.getElementById('n5').innerHTML == '-' && document.getElementById('n7').innerHTML == '-')
{
kwicR();
}

if (document.zabba.chooser.value != 'collocates')
{
// document.all.w2.value = '';
// document.zabba.sortBy.options[0].selected = true;
// document.zabba.sortBy.options[1].selected = false;
// document.zabba.minfreq1.options[0].selected = true;
// document.zabba.minfreq1.options[1].selected = false;
// document.zabba.limfreq1.checked = false;
}

if (document.all.wl.value == 0 && document.all.wr.value == 0)
{
// document.all.label1.style.backgroundColor='#EEF3F9';
document.getElementById('cellL1').style.backgroundColor='#A6FFA6';
document.getElementById('cellL2').style.backgroundColor='#A6FFA6';
document.getElementById('cellL3').style.backgroundColor='#A6FFA6';
document.getElementById('cellL4').style.backgroundColor='#A6FFA6';
document.getElementById('cellR1').style.backgroundColor='#A6FFA6';
document.getElementById('cellR2').style.backgroundColor='#A6FFA6';
document.getElementById('cellR3').style.backgroundColor='#A6FFA6';
document.getElementById('cellR4').style.backgroundColor='#A6FFA6';
// document.all.cellL1.style.backgroundColor='#A6FFA6';
// document.all.cellL2.style.backgroundColor='#A6FFA6';
// document.all.cellL3.style.backgroundColor='#A6FFA6';
// document.all.cellL4.style.backgroundColor='#A6FFA6';
// document.all.cellR1.style.backgroundColor='#A6FFA6';
// document.all.cellR2.style.backgroundColor='#A6FFA6';
// document.all.cellR3.style.backgroundColor='#A6FFA6';
// document.all.cellR4.style.backgroundColor='#A6FFA6';
document.all.wl.value = 4;
document.all.wr.value = 4;
}

if (document.zabba.chooser.value == 'compareWords' && document.getElementById('w2').value == '')
{
// document.all.w2.value = '*';
}


if (document.zabba.chooser.value == 'compareWords')
{
document.getElementById('w1a').value = document.getElementById('p').value;
}

if (document.zabba.chooser.value == "kwic" && document.getElementById('s1').value == 0 && document.getElementById('s2').value == 0 && document.getElementById('s3').value == 0)
{
document.all.cellc5.style.backgroundColor='#A6FFA6';
document.all.cellc6.style.backgroundColor='#A6FFA6';
document.all.cellc7.style.backgroundColor='#A6FFA6';
document.getElementById('n5').innerHTML = "1";
document.getElementById('n6').innerHTML = "2";
document.getElementById('n7').innerHTML = "3";
document.getElementById('s1').value = 1;
document.getElementById('s2').value = 2;
document.getElementById('s3').value = 3;
document.zabba.counter.value = 4;
}
// alert(top.x1.document.zabba.target);
}

