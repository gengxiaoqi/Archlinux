function toggleHelp()
{
if (document.all.labelHelp.innerHTML=='(Show help)')
{
document.all.labelHelp.innerHTML='(Hide help)'
// document.all.labelInterface.innerHTML='New interface'
// document.all.labelHelp1.innerHTML='Help'
document.all.helpAll.style.display='inline';
}
else
{
document.all.labelHelp.innerHTML='(Show help)'
// document.all.labelInterface.innerHTML=''
// document.all.labelHelp1.innerHTML=''
document.all.helpAll.style.display='none';
}
}



function sortByChanged()

{
if (document.zabba.sortBy.options[1].selected == true) // RELEVANCE

{
if (document.zabba.sec2.options[0].selected == true)
{
document.zabba.minfreq1.options[0].selected = true;
document.all.xrelevance.style.visibility='visible';

// document.getElementById('xrelevance').innerHTML = "<b>MUT INFO</b>";

document.zabba.minfreq1.style.backgroundColor='#A6FFA6';
// document.zabba.freq1.value = '10';
document.zabba.freq1.style.backgroundColor='#A6FFA6';
document.zabba.limfreq1.checked = true;
document.zabba.limfreq1.style.backgroundColor='#A6FFA6';
}
} // END RELEVANCE

else  // FREQUENCY
{
document.zabba.limfreq1.checked = false;
document.getElementById('xrelevance').style.visibility='hidden';
document.zabba.minfreq1.style.backgroundColor='#FFFFFF';
document.zabba.freq1.style.backgroundColor='#FFFFFF';
document.zabba.limfreq1.style.backgroundColor='#E2ECF5';
} // END FREQUENCY
}



