document.all.sortLabel.style.padding='3px 8px';
document.all.sortLabel.style.backgroundColor='#E0E0E0';
// document.all.sortLabel.style.fontWeight = "bold";
// document.all.sortLabel.style.borderWidth='2px';
// document.all.sortLabel.style.borderStyle='solid';
// document.all.sortLabel.style.borderColor='black';

document.all.sortLimit2.style.display='inline';



top.x1.document.all.helpSort.style.display='inline'
top.x1.document.all.helpSec.style.display='none';
top.x1.document.all.helpOpt.style.display='none';
top.x1.document.all.helpVC.style.display='none';
