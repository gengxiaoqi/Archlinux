function toggleSort()
{



// document.all.sortLimit2.style.display='none';
document.all.VC2.style.display='none';
document.all.options2.style.display='none';

document.all.sections2.style.display='none';

/*
document.all.sortLabel.style.color='#808080';
document.all.sortLabel.style.padding='0px';
document.all.sortLabel.style.backgroundColor='#EEF3F9';
document.all.sortLabel.style.fontWeight = "normal";
document.all.sortLabel.style.borderWidth='0px';
document.all.sortLabel.style.borderStyle='none';
document.all.sortLabel.style.borderColor='black';
*/

document.all.optionsLabel.style.color='#808080';
document.all.optionsLabel.style.padding='0px';
document.all.optionsLabel.style.backgroundColor='#EEF3F9';
document.all.optionsLabel.style.fontWeight = "normal";
document.all.optionsLabel.style.borderWidth='0px';
document.all.optionsLabel.style.borderStyle='none';
document.all.optionsLabel.style.borderColor='black';


document.all.sectionsLabel.style.color='#808080';
document.all.sectionsLabel.style.padding='0px';
document.all.sectionsLabel.style.backgroundColor='#EEF3F9';
document.all.sectionsLabel.style.fontWeight = "normal";
document.all.sectionsLabel.style.borderWidth='0px';
document.all.sectionsLabel.style.borderStyle='none';
document.all.sectionsLabel.style.borderColor='black';


document.all.VCLabel.style.color='#808080';
document.all.VCLabel.style.padding='0px';
document.all.VCLabel.style.backgroundColor='#EEF3F9';
document.all.VCLabel.style.fontWeight = "normal";
document.all.VCLabel.style.borderWidth='0px';
document.all.VCLabel.style.borderStyle='none';
document.all.VCLabel.style.borderColor='black';

