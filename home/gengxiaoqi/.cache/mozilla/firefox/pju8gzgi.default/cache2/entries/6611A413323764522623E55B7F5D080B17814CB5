<script language="Javascript">

function toggleSec1Color()
{
// alert(top.x1.document.all.sec1.style.backgroundColor);
// top.x1.document.all.secVC.style.backgroundColor = '#00FF00';
}

function vcdo1()
{
top.x1.document.all.secVC.style.backgroundColor = '#00FF00'

top.x1.document.zabba.secVC.options[0].selected = true;
top.x1.document.zabba.secVC.options[1].selected = false;
top.x1.document.all.sectionsLabel.style.color='#808080';
top.x1.document.all.sectionsLabel.style.padding='0px';
top.x1.document.all.sectionsLabel.style.backgroundColor='#EEF3F9';
top.x1.document.all.sectionsLabel.style.fontWeight = "normal";
top.x1.document.all.sectionsLabel.style.borderWidth='0px';
top.x1.document.all.sectionsLabel.style.borderStyle='none';
top.x1.document.all.sectionsLabel.style.borderColor='black';

top.document.getElementById('myFrames').cols="*,0,0,0)";

}

function vcdo3()
{
top.x1.document.zabba.secVC.options[1].selected = true;
top.x1.document.zabba.secVC.options[0].selected = false;
top.x1.document.all.secVC.style.backgroundColor = '#00FF00'

top.x1.document.all.sectionsLabel.style.color='#808080';
top.x1.document.all.sectionsLabel.style.padding='0px';
top.x1.document.all.sectionsLabel.style.backgroundColor='#EEF3F9';
top.x1.document.all.sectionsLabel.style.fontWeight = "normal";
top.x1.document.all.sectionsLabel.style.borderWidth='0px';
top.x1.document.all.sectionsLabel.style.borderStyle='none';
top.x1.document.all.sectionsLabel.style.borderColor='black';

}

function vcdo1a()
{
top.x1.document.all.secVC.style.backgroundColor = '#FFFFFF';
}

function vcdo2a()
{
top.x1.document.all.sortBy.style.backgroundColor = '#FFFFFF'
top.x1.document.all.limfreq1.style.backgroundColor = '#FFFFFF'
top.x1.document.all.freq1.style.backgroundColor = '#FFFFFF'
}

function vcdo2()
{
top.x1.document.all.sortLimit2.style.display='inline';
top.x1.document.all.VC2.style.display='none';
top.x1.document.zabba.sortBy.options[0].selected = false;
top.x1.document.zabba.sortBy.options[1].selected = true;
top.x1.document.zabba.limfreq1.checked = true;
top.x1.document.zabba.freq1.value = '7';
top.x1.document.all.sortBy.style.backgroundColor = '#00FF00'
top.x1.document.all.limfreq1.style.backgroundColor = '#00FF00'
top.x1.document.all.freq1.style.backgroundColor = '#00FF00'
top.x1.document.all.sortLabel.style.color='#000000';
top.x1.document.all.sortLabel.style.padding='3px 8px';
top.x1.document.all.sortLabel.style.backgroundColor='#E0E0E0';
}

